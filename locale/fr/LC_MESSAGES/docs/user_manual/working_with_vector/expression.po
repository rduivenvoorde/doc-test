msgstr "abs"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "Returns the absolute value of a number"
msgstr "Renvoie la valeur absolue d'un nombre"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "acos"
msgstr "acos"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "Returns the inverse cosine of a value in radians"
msgstr "Renvoie le cosinus inverse d'une valeur en radians"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "asin"
msgstr "asin"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "Returns the inverse sine of a value in radians"
msgstr "Renvoie le sinus inverse d'une valeur en radians"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
msgid "atan"
msgstr "atan"

#: ../../docs/user_manual/working_with_vector/expression.rst:1
